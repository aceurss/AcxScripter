-- Islands
local IslandsTab = Window:NewTab("Islands")
local IslandsSection = IslandsTab:NewSection("Teleport & Unlock")

IslandsSection:NewButton("Unlock All Islands", "Unlocks all islands in one click", function()
    print("Unlocking all islands...")
    -- Add Unlock All Islands functionality here
end)

IslandsSection:NewDropdown("Teleport to Island", {"Island 1", "Island 2", "Island 3", "Island 4", "Island 5", "Island 6", "Island 7"}, function(selected)
    print("Teleporting to " .. selected)
    -- Add Teleport functionality here
end)

-- Settings
local SettingsTab = Window:NewTab("Settings")
