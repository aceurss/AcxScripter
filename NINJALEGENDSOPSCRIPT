-- Roblox Auto Farm GUI Script

local Library = loadstring(game:HttpGet("https://pastebin.com/raw/yourlibrarylink"))()
local Window = Library.CreateLib("My Script", "DarkTheme")

-- Home
local HomeTab = Window:NewTab("Home")
local HomeSection = HomeTab:NewSection("Welcome To My Script!")

-- Main
local MainTab = Window:NewTab("Main")
local MainSection = MainTab:NewSection("Auto Farm Features")

MainSection:NewToggle("Auto Sell", "Auto Sells All Ninjitsu", function(state)
    if state then
        print("Auto Sell Enabled")
        -- Add Auto Sell functionality here
    else
        print("Auto Sell Disabled")
    end
end)

